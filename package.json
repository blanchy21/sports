{
  "name": "sports",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:force": "./scripts/start-dev.sh",
    "postinstall": "prisma generate",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start --port 3000",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "jest --config jest.config.ts",
    "test:unit": "jest --config jest.config.ts",
    "test:unit:watch": "jest --config jest.config.ts --watch",
    "test:workerbee": "npx tsx scripts/test-workerbee.ts",
    "test:workerbee:account": "npx tsx scripts/test-workerbee.ts account",
    "test:workerbee:content": "npx tsx scripts/test-workerbee.ts content",
    "test:workerbee:posting": "npx tsx scripts/test-workerbee.ts posting",
    "test:workerbee:voting": "npx tsx scripts/test-workerbee.ts voting",
    "test:workerbee:comments": "npx tsx scripts/test-workerbee.ts comments",
    "test:workerbee:integration": "RUN_WORKERBEE_TESTS=true jest --config jest.config.ts tests/workerbee/workerbee.integration.test.ts",
    "benchmark:workerbee": "npx tsx scripts/benchmark-workerbee.ts",
    "test:optimizations": "npx tsx scripts/test-optimizations.ts",
    "test:node-health": "npx tsx scripts/test-node-health.ts",
    "monitor:workerbee": "npx tsx scripts/monitor-workerbee.ts",
    "monitor:workerbee:watch": "npx tsx scripts/monitor-workerbee.ts continuous",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aioha/aioha": "^1.8.0",
    "@aioha/react-ui": "^1.8.0",
    "@hiveio/dhive": "^1.3.2",
    "@hiveio/wax": "^1.28.4-rc0",
    "@hiveio/workerbee": "^1.28.4-rc0",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@sentry/nextjs": "^10.33.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.5",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.14.0",
    "framer-motion": "^12.23.24",
    "immer": "^11.1.3",
    "isomorphic-dompurify": "^3.0.0",
    "lru-cache": "^10.4.3",
    "lucide-react": "^0.545.0",
    "next": "^15.5.4",
    "next-auth": "^4.24.13",
    "pg": "^8.18.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.9",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^2.0.16",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.4.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "undici": "^7.18.2"
  }
}
